{
  "name": "@netless/fastboard",
  "version": "0.3.15",
  "description": "A tiny wrapper of white-web-sdk and @netless/window-manager.",
  "main": "dist/index.js",
  "files": [
    "src",
    "dist",
    "lite.d.ts"
  ],
  "repository": "netless-io/fastboard",
  "dependencies": {
    "@netless/fastboard-core": "0.3.15",
    "@netless/fastboard-ui": "0.3.15"
  },
  "devDependencies": {
    "@netless/esbuild-plugin-inline-sass": "0.1.0",
    "@netless/buildtool": "0.1.0"
  },
  "scripts": {
    "cleanup": "rimraf dist",
    "check": "tsc --noEmit",
    "build": "buildtool",
    "dev": "vite"
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./lite": {
      "types": "./dist/lite.d.ts",
      "default": "./dist/lite.mjs"
    }
  }
}